    "schedule": {
        "since": "2020-04-01 00:00:00",
        "until": "2020-04-30 23:59:59",
        "limitation": {
            "pre-entry": "2020-01-31 23:59:59",
            "entry": "2020-02-10 23:59:59",
            "submit": "2020-02-29 23:59:59",
            "catalog": "2020-03-31 23:59:59",
            "final_submit": "2020-03-31 23:59:59"
        }
    },
    "question": {
        "prologue": [
            "バーチャルフロンティアへエントリー頂き、ありがとうございました。",
            "まだ参加登録は完了しておりません。",
            "これからいくつかの質問にお答え頂きます。",
            "対話形式になっていますので、質問の答えを書き込んでください。",
            "サーバーから回答に:thumbsup:のリアクションが付き、質問のメッセージが更新されていれば、正しく回答できています。",
            "もし回答を間違えてしまった場合は、回答したチャットを直接編集してください。",
            "また、回答したチャットを削除してしまった場合は、一通り回答を終えた後に`+answer <問題番号> <問題の答え>`と",
            "すべての質問に回答した後、参加登録を完了できます。",
            "参加登録を完了した後は、原則的に回答を修正できませんので、",
            "--------------------------------------------------------"
        ],
        "reqire": [
            {
                "header": "サークル名",
                "description": "サークルの名前を入力します。",
                "type": "string",
                "length": 32,
                "nargs": [
                    1
                ]
            },
            {
                "header": "ブースのジャンル",
                "description": "おおまかなジャンルを、選択肢の中から選んで記入してください。",
                "type": "id-choice",
                "choice": [
                    "かっこいい系アバター",
                    "かわいい系アバター",
                    "アクセサリー",
                    "ネタ",
                    "その他"
                ],
                "nargs": [
                    1
                ]
            },
            {
                "header": "合同サークル申請",
                "description": "合同サークルを申請しますか？",
                "type": "switch",
                "key": "combi"
            },
            {
                "header": "合同参加者",
                "description": [
                    "合同サークルとして参加する、他サークルの代表者を記入してください。",
                    "他サークルの代表者もこのサーバーに参加している必要があります。",
                    "（エントリーの必要はありません。代表者のみがエントリー処理を行ってください。）",
                    "記入時は`@xxx#8888`のメンション形式で記入してください。"
                ],
                "type": "userlist",
                "nargs": [
                    1,
                    2,
                    3,
                    4
                ],
                "combi": true
            },
            {
                "header": "使用スペース数",
                "description": [
                    "使用するスペース数を決定できます。個人サークルの場合は、最大2つまで申請することができます。",
                    "1つのスペースサイズは3辺5mの立方体空間です。"
                ],
                "type": "integer",
                "min": 1,
                "max": 4,
                "nargs": [
                    1
                ],
                "combi": true
            },
            {
                "header": "使用スペース数",
                "description": [
                    "使用するスペース数を決定できます。合同サークルの場合は、最大4つまで申請することができます。",
                    "1つのスペースサイズは3辺5mの立方体空間です。"
                ],
                "type": "integer",
                "min": 1,
                "max": 4,
                "nargs": [
                    1
                ],
                "combi": false
            }
        ],
        "optional": [
            {
                "text": "隣接配置希望",
                "description": [
                    "隣接して配置を希望する場合は、どのサークルと隣接させたいかを回答してください。",
                    "サークルの指定はサークル名で、最大2サークルまで（両隣）、1サークル1行で入力してください。",
                    "必ず、相互指定が必要です。",
                    "つまり、指定した相手方の「隣接配置希望」の質問回答として、あなたのサークル名が含まれている必要があります。"
                ],
                "type": "string",
                "nargs": [
                    1,
                    2
                ],
                "limit_phase": "submit"
            },
            {
                "text": "サークルカット",
                "description": [
                    "ウェブサイトへ掲載するものです。",
                    "問題の答えの代わりに、画像を添付してください。",
                    "画像はPNG、もしくはJPEG形式で、512(px) x 512(px)の正方形画像を推奨します。"
                ],
                "nargs": [
                    1
                ],
                "type": "picture",
                "limit_phase": "catalog"
            },
            {
                "text": "Twitter ID",
                "description": [
                    "ウェブサイトへ掲載するものです。",
                    "任意で回答頂けますので、Twitterの開設をお願いするものではありません。",
                    "@を抜いたIDのみを回答ください。（最大2つまで、1つにつき1行で回答）"
                ],
                "type": "regex",
                "nargs": [
                    1,
                    2
                ],
                "regex_rule": "[a-zA-Z0-9]+",
                "limit_phase": "catalog"
            },
            {
                "text": "Pixiv Booth URL",
                "description": [
                    "ウェブサイトへ掲載するものです。",
                    "商品ページではなく、出展者様の店舗ページへのリンクをご回答ください。"
                ],
                "type": "regex",
                "regex_rule": "(https://)?[a-zA-Z0-9\\-.]+\\.booth.pm/?",
                "nargs": [
                    1
                ],
                "limit_phase": "catalog"
            },
            {
                "text": "カタログ登録品1",
                "description": "詳しくは http://vfrontier.work/howto/entry/catalog.html を参照ください",
                "type": "catalog",
                "nargs": [
                    1
                ],
                "limit_phase": "catalog"
            },
            {
                "text": "カタログ登録品2",
                "description": "詳しくは http://vfrontier.work/howto/entry/catalog.html を参照ください",
                "type": "catalog",
                "nargs": [
                    1
                ],
                "limit_phase": "catalog"
            },
            {
                "text": "カタログ登録品3",
                "description": "詳しくは http://vfrontier.work/howto/entry/catalog.html を参照ください",
                "type": "catalog",
                "nargs": [
                    1
                ],
                "limit_phase": "catalog"
            }
        ]
